<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ule.fenxiao.dao.UserMapper">
    <resultMap id="BaseResultMap" type="com.ule.fenxiao.vo.UserInfo">
        <id column="id" property="id" jdbcType="NUMERIC"/>
        <result column="user_info_name" property="userInfoName" jdbcType="VARCHAR"/>
        <result column="user_info_address" property="userInfoAddress" jdbcType="VARCHAR"/>
        <result column="user_info_email" property="userInfoEmail" jdbcType="VARCHAR"/>
        <result column="user_info_tel" property="userInfoTel" jdbcType="VARCHAR"/>
    </resultMap>
    <select id="queryAll" resultMap="BaseResultMap">
        select * from user_info
    </select>
</mapper>